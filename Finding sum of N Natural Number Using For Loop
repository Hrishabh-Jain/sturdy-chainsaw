#include<iostream>
using namespace std;
int main()
{
    
    int num;
    cin>>num;
     int sum = 0;
    cout<<"Finding sum of first n natural numbers"<<endl;
    cout<<"First Ten naural numbers are"<<endl;
    for(int i = 0; i<=num ; i++)
    {
       
       
       cout<<i<<" "<<endl;
       
        
        sum = sum +i;
    }
    cout<<"Sum of natural number =" <<sum<<endl;
    
    
    
    
    
    
    
    
    
}
