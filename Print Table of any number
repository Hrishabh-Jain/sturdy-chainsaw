int main() {
    // Write C++ code here
    int n ;
    cout<<"Give the value of n"<<endl;
    cin>>n;
  for(int i =1;  i<=10; i++)
  {
      cout<<"number" <<i<<"\t=\t"<< n *i<<endl;
  }
  cout<<endl;




    return 0;
}
