#include<iostream>
using namespace std;
int main()
{
    
    int n ;
cout<<"Please give number for chechking"<<n<<endl;
cin>>n;
cout<<"Check the weather the number is prime or not prime "<<endl;
cout<<"-----------------------------------------------       "<<endl;

for(int i =0; i<=1; )
{
    if(n%2 == 0)
    {
        cout<<"Not Prime"<<endl;
    }
    else
    {
        cout<<"Prime"<<endl;
    }
    break;
}
    
    
    
    
    
    
    
    
    
    
}
